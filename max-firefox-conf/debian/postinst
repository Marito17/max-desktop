#!/bin/sh

set -e

case "$1" in
    configure)


    if [ -e /usr/share/pixmaps/firefox-3.0.png ]; then
       if [ ! -e /usr/share/pixmaps/firefox.png ]; then
         ln -s /usr/share/pixmaps/firefox-3.0.png /usr/share/pixmaps/firefox.png || true
       fi
    fi

    ;;

esac


#DEBHELPER#

exit 0


